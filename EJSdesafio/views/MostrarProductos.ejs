<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <%- include("Templates/Cabecer") %>
    <h1 class="titulo">Productos Agregados</h1>

    <div class="productosContainer">
      <% lista.forEach(element => { %>
      <div class="Producto">
        <p class="nombreProducto"><%=element.nombre %></p>
        <p class="precioProducto"><%=element.precio %>$</p>
        <div class="imgProducto">
          <img src="<%=element.img %>" alt="" class="img" />
        </div>
      </div>
      <%}) %>
    </div>
  </body>
</html>
